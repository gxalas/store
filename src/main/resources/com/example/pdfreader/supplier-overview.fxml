<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.pdfreader.Controllers.SupplierOverView">
   <children>
      <VBox prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox />
            <HBox>
               <children>
                  <VBox alignment="CENTER">
                     <children>
                        <ComboBox fx:id="cbSuppliers" prefWidth="150.0">
                           <VBox.margin>
                              <Insets bottom="12.0" top="10.0" />
                           </VBox.margin>
                        </ComboBox>
                        <TableView fx:id="tableSupplierProducts">
                          <columns>
                            <TableColumn prefWidth="75.0" text="C1" />
                            <TableColumn prefWidth="75.0" text="C2" />
                          </columns>
                        </TableView>
                        <Button fx:id="btnUnlink" mnemonicParsing="false" text="Unlink">
                           <VBox.margin>
                              <Insets bottom="15.0" top="10.0" />
                           </VBox.margin>
                        </Button>
                     </children>
                  </VBox>
                  <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
                     <children>
                        <Button fx:id="btnLink" mnemonicParsing="false" text="&lt;- Link" />
                     </children>
                  </VBox>
                  <VBox alignment="CENTER">
                     <children>
                        <ComboBox fx:id="cbDepartment" prefWidth="150.0">
                           <VBox.margin>
                              <Insets bottom="12.0" top="10.0" />
                           </VBox.margin>
                        </ComboBox>
                        <TableView fx:id="tableFreeProducts">
                          <columns>
                            <TableColumn prefWidth="75.0" text="C1" />
                            <TableColumn prefWidth="75.0" text="C2" />
                          </columns>
                        </TableView>
                     </children>
                  </VBox>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
